void DroneBahavior(){

	if(Vector3D.Distance(myPosition, despawnPosition) &lt; 150){
		
		var tryDespawn = AttemptDespawn();
		
	}
	
	if(iceRefillTimer &gt;= iceRefillTimerTrigger){
		
		iceRefillTimer = 0;
		var tryIceFill = FillGasGenerators();
		
		foreach(var weapon in weaponList){
			
			if(weapon == null){
				
				continue;
				
			}
			
			if(weapon.IsFunctional == false/* || weapon.GetInventory(0).GetItems().Count &gt; 0*/){
				
				continue;
				
			}
			
			var ammofill = GetAmmoAndFillWeapon(weapon.EntityId, 20);
			
		}
		
	}else{
		
		iceRefillTimer++;
		
	}
	
	if(TurretsEngagingTarget() == true){
		
		currentMode = "EngageTargets";
		
	}
	
	if(weaponsFiring == true){
		
		weaponsFiring = false;
		WeaponFireMode(false, remoteControl.WorldMatrix.Forward, weaponList);
		
	}
		
	if(currentMode == "Drift" || currentMode == "Retreat"){
		
		scanTimer++;
		
		if(scanTimer &gt;= scanTimerTrigger){
			
			scanTimer = 0;
			var allNearbyTargets = GetAllEnemyGrids("None", scanDistance);
			allBroadcastingTargets.Clear();
			allMovingTargets.Clear();
			
			foreach(var id in allNearbyTargets){
				
				if(TargetIsBroadcasting(id) == true){
					
					allBroadcastingTargets.Add(id);
					
				}
				
				if(Vector3D.Distance(GetTrackedEntityPosition(id), myPosition) &lt; scanDistance / 2){
					
					if(NoticableMovingTarget(id) == true &amp;&amp; TargetPowered(id) == true){
						
						allMovingTargets.Add(id);
						
					}
					
				}
				
			}
			
			if(allMovingTargets.Count &gt; 0){
				
				var notify = BroadcastNotification(movementDetectNotification, 6000, "Red", despawnDistance);
				currentMode = "CheckNextTargetPosition";
				notificationBroadcast = true;
				
			}else if(allBroadcastingTargets.Count &gt; 0){
				
				var notify = BroadcastNotification(signalDetectNotification, 6000, "Red", despawnDistance);
				currentMode = "CheckNextTargetPosition";
				notificationBroadcast = true;
				
			}else{
				
				currentMode = "Retreat";
				AutoPilotController(remoteControl, true, despawnPosition, false, 25);
				
			}
			
		}
		
	}
	
	if(currentMode == "CheckNextTargetPosition"){
		
		bool targetAssigned = false;
		
		if(allMovingTargets.Count &gt; 0){
			
			currentTarget = allMovingTargets[0];
			targetAssigned = true;
			
		}
		
		if(allBroadcastingTargets.Count &gt; 0 &amp;&amp; targetAssigned == false){
			
			currentTarget = allBroadcastingTargets[0];
			targetAssigned = true;
			
		}
		
		if(targetAssigned == false){
			
			scanTimer = scanTimerTrigger;
			currentMode = "Drift";
			return;
			
		}
		
		currentCheckPosition = GetUpdatedTargetCoords(currentTarget);
		
		currentMode = "CheckTarget";
		
	}
	
	if(currentMode == "CheckTarget"){
				
		signalScanRefresh++;
		bool canUpdateTarget = false;
		
		if(signalScanRefresh &gt;= signalScanRefreshTrigger || Vector3D.Distance(myPosition, currentCheckPosition) &lt; 100){
			
			signalScanRefresh = 0;
			
			if(GetTargetShipSystem(currentTarget, "Power") == 0){
			
				scanTimer = scanTimerTrigger;
				currentMode = "Drift";
				return;
				
			}
			
			if(TargetIsBroadcasting(currentTarget) == true){
					
				canUpdateTarget = true;
				
			}
			
			if(Vector3D.Distance(GetTrackedEntityPosition(currentTarget), myPosition) &lt; scanDistance / 2){
				
				if(NoticableMovingTarget(currentTarget) == true &amp;&amp; TargetPowered(currentTarget) == true){
					
					canUpdateTarget = true;
					
				}
				
			}
			
		}
		
		if(canUpdateTarget == true){
			
			currentCheckPosition = GetUpdatedTargetCoords(currentTarget);
			
		}
		
		AutoPilotController(remoteControl, true, currentCheckPosition, true, 100);
		
		if(Vector3D.Distance(myPosition, currentCheckPosition) &lt; 100){
			
			var scannedData = CameraRaycastTarget(currentCheckPosition, cameraList);
			
			if(scannedData.IsEmpty() == false){
				
				currentMode = "EngageTargets";
				
			}else{
				
				currentMode = "Drift";
				
			}

		}
		
	}
	
	if(currentMode == "EngageTargets"){
		
		currentTargetPosition = GetTrackedEntityPosition(currentTarget);
		
		if(currentTargetPosition == Vector3D.Zero){
			
			currentMode = "Drift";
			return;
			
		}
		
		signalScanRefresh++;
		
		if(signalScanRefresh &gt; signalScanRefreshTrigger){
			
			signalScanRefresh = 0;
			
			if(GetTargetShipSystem(currentTarget, "Power") == 0){
				
				currentMode = "Drift";
				return;
				
			}
			
		}

		var targetDistance = Vector3D.Distance(myPosition, currentTargetPosition);
		
		if(targetDistance &lt; scanDistance){
			
			var attackCoords = Vector3D.Zero;
			
			if(inNaturalGravity == false){
				
				attackCoords = CreateDirectionAndTarget(currentTargetPosition, myPosition, currentTargetPosition, targetEngageDistance);
				
				
			}else{
				
				attackCoords = CreateDirectionAndTarget(planetPosition, currentTargetPosition, currentTargetPosition, targetEngageDistance);
				
			}
			
			AutoPilotController(remoteControl, true, attackCoords, true, 100);
			
			if(ApproximateTarget(currentTargetPosition, myPosition, remoteControl.WorldMatrix.Forward, 4) == true){
				
				weaponsFiring = true;
				WeaponFireMode(true, remoteControl.WorldMatrix.Forward, weaponList);
				
				if(hasHarpoon == true &amp;&amp; harpoonFired == false &amp;&amp; harpoonMergeBlock != null){
					
					foreach(var grav in gravGenList){
						
						if(grav != null){
							
							grav.Enabled = true;
							
						}
						
					}
					
					if(harpoonGyro != null){
						
						harpoonGyro.Enabled = true;
						
					}
					
					harpoonMergeBlock.Enabled = false;
					harpoonFired = true;
					
				}
				
			}
			
		}else{
			
			currentMode = "Drift";
			
		}
		
	}
	
}

void Main(){
	
	if(scriptSetup == false){
		
		scriptSetup = true;
		Setup();
		
	}
	
	if(scriptInitFailed == true){
		
		Echo("Debug: Script Init Fail");
		return;
		
	}
	
	tickCounter += tickIncrement;
	
	if(tickCounter &lt; 60){
		
		return;
		
	}
		
	tickCounter = 0;
	
	if(CheckIfNPC() == false){
		
		Echo("Non-NPC. Aborting Script Execution");
		return;
		
	}
	
	//Get Basic Positions And Distances
	myPosition = remoteControl.GetPosition();
	inNaturalGravity = remoteControl.TryGetPlanetPosition(out planetPosition);
	inNaturalGravity = remoteControl.TryGetPlanetElevation(MyPlanetElevation.Surface, out planetAltitude);
		
	DroneBahavior();
	
}

void Setup(){
	
	//Process Faction
	bool alreadyNPC = false;
	
	if(useCustomFaction == true &amp;&amp; myFactionTag != ""){
		
		alreadyNPC = CheckIfNPC(myFactionTag);
		
	}else{
		
		alreadyNPC = CheckIfNPC();
		
	}

	if(alreadyNPC == false){
		
		Echo("NPC Change");
		var changeFaction = ChangeNPCFaction(myFactionTag);
		
		if(changeFaction == true){
			
			return;
			
		}
		
	}
	
	//Get Remote
	if(preferredRemoteControl != ""){
		
		remoteControl = GridTerminalSystem.GetBlockWithName(preferredRemoteControl) as IMyRemoteControl;
		
	}

	if(remoteControl == null){
		
		var remoteList = new List&lt;IMyRemoteControl&gt;();
		GridTerminalSystem.GetBlocksOfType&lt;IMyRemoteControl&gt;(remoteList);

		foreach(var remote in remoteList){

			if(remote.IsFunctional == true){
				
				remoteControl = remote;
				break;
				
			}
			
		}
	
		if(remoteControl == null){
			
			scriptInitFailed = true;
			return;
			
		}else{
			
			if(remoteControl.IsFunctional == false){
				
				scriptInitFailed = true;
				return;
				
			}
			
		}
		
	}else{
		
		if(remoteControl.IsFunctional == false){
				
			scriptInitFailed = true;
			return;
			
		}
		
	}
	
	//Calculate Despawn
	despawnPosition = 12000 * remoteControl.WorldMatrix.Forward + remoteControl.GetPosition();
	
	//Get Weapons
	GridTerminalSystem.GetBlocksOfType&lt;IMyUserControllableGun&gt;(weaponList);
	
	//Get Cameras
	GridTerminalSystem.GetBlocksOfType&lt;IMyCameraBlock&gt;(cameraList);
	
	if(hasHarpoon == true){
		
		harpoonMergeBlock = GridTerminalSystem.GetBlockWithName(harpoonMergeBlockName) as IMyShipMergeBlock;
		harpoonGyro = GridTerminalSystem.GetBlockWithName(harpoonGyroName) as IMyGyro;
		GridTerminalSystem.GetBlocksOfType&lt;IMyGravityGenerator&gt;(gravGenList);
		
		if(harpoonMergeBlock != null){
			
			if(harpoonMergeBlock.IsFunctional == false){
				
				hasHarpoon = false;
				
			}
			
		}
		
	}
	
}

bool ApproximateTarget(Vector3D targetCoords, Vector3D myShipPosition, Vector3D myShipForward, double maxAccuracyVariance, double maxTargetDistance = 850){
	
	double distanceToTarget = Vector3D.Distance(targetCoords, myShipPosition);
	
	if(distanceToTarget &gt; maxTargetDistance){
		
		return false;
		
	}
	
	var impactPosition = distanceToTarget * myShipForward + myShipPosition;
	var accuracyDistance = Vector3D.Distance(impactPosition, targetCoords);

	if(accuracyDistance &lt; maxAccuracyVariance){
		
		return true;
		
	}
	
	return false;
	
}

void AutoPilotController(IMyRemoteControl remoteControlAP, bool enableAutoPilot, Vector3D coords, bool collisionModeEnable = false, float speedLimit = 100){
	
	if(remoteControlAP == null){
		
		return;
		
	}
	
	if(remoteControlAP.IsFunctional == false){
		
		return;
		
	}
	
	remoteControlAP.ClearWaypoints();
	remoteControlAP.AddWaypoint(coords, "Destination");
	remoteControlAP.FlightMode = FlightMode.OneWay;
	remoteControlAP.SetAutoPilotEnabled(enableAutoPilot);
	remoteControlAP.SetCollisionAvoidance(collisionModeEnable);
	remoteControlAP.SpeedLimit = speedLimit;
	
}

Vector3D CreateDirectionAndTarget(Vector3D startDirCoords, Vector3D endDirCoords, Vector3D startPathCoords, double pathDistance){
	
	var direction = Vector3D.Normalize(endDirCoords - startDirCoords);
	var coords = direction * pathDistance + startPathCoords;
	return coords;
	
}

MyDetectedEntityInfo CameraRaycastLinear(Vector3D direction, float scanDistance = 2000, List&lt;IMyCameraBlock&gt; cameraRaycastList = null){
		
	MyDetectedEntityInfo scanResults = new MyDetectedEntityInfo();
	
	if(cameraRaycastList == null){
		
		return scanResults;
		
	}
	
	foreach(var camera in cameraRaycastList){

		if(camera == null){
			
			continue;
			
		}
		
		if(camera.IsFunctional == false || camera.IsWorking == false){
			
			continue;
			
		}

		if(camera.WorldMatrix.Forward != direction &amp;&amp; direction != Vector3D.Zero){
			
			continue;
			
		}
		
		camera.EnableRaycast = true;
				
		if(camera.CanScan(scanDistance) == false){
			
			continue;
			
		}
		
		scanResults = camera.Raycast(scanDistance, 0, 0);
		
		if(scanResults.IsEmpty() == false){
			
			return scanResults;
			
		}
		
	}
	
	return scanResults;
	
}

MyDetectedEntityInfo CameraRaycastTarget(Vector3D target, List&lt;IMyCameraBlock&gt; cameraRaycastList = null){

	MyDetectedEntityInfo scanResults = new MyDetectedEntityInfo();
	
	if(cameraRaycastList == null){
		
		return scanResults;
		
	}
	
	foreach(var camera in cameraRaycastList){

		if(camera == null){
			
			continue;
			
		}
		
		if(camera.IsFunctional == false || camera.IsWorking == false){
			
			continue;
			
		}

		camera.EnableRaycast = true;
				
		if(camera.CanScan(target) == false){
			
			continue;
			
		}
		
		scanResults = camera.Raycast(target);
		
		if(scanResults.IsEmpty() == false){
			
			return scanResults;
			
		}
		
	}
	
	return scanResults;
	
}

bool TurretsEngagingTarget(){
	
	foreach(var weapon in weaponList){
		
		if(weapon == null){
			
			continue;
			
		}
		
		if(weapon.IsFunctional == false || weapon.IsShooting == false || weapon as IMyLargeTurretBase == null){
			
			continue;
			
		}
		
		var turret = weapon as IMyLargeTurretBase;
		var scannedData = turret.GetTargetedEntity();
		
		if(scannedData.IsEmpty() == true){
			
			continue;
			
		}
		
		currentTarget = scannedData.EntityId;
		return true;
		
	}
	
	return false;
	
}

void WeaponFireMode(bool fireWeapons, Vector3D weaponDirection, List&lt;IMyUserControllableGun&gt; fixedWeaponList = null){
	
	if(fixedWeaponList == null){
		
		return;
		
	}
	
	foreach(var weapon in fixedWeaponList){
		
		if(weapon == null){
			
			continue;
			
		}
		
		if(weapon as IMyLargeTurretBase != null){
			
			continue;
			
		}
		
		if(weapon.IsFunctional == false || weapon.IsWorking == false || weapon.WorldMatrix.Forward != weaponDirection){
			
			continue;
			
		}
		
		if(fireWeapons == true){
			
			weapon.ApplyAction("Shoot_On");
			
		}else{
			
			weapon.ApplyAction("Shoot_Off");
			
		}
		
	}
	
}

//Mixed Methods

Vector3D GetUpdatedTargetCoords(long targetId){
	
	var targetCoords = GetTrackedEntityPosition(targetId);
	
	if(targetCoords == Vector3D.Zero){
		
		return targetCoords;
		
	}
	
	var targetCheckMatrix = remoteControl.WorldMatrix; //temp value
	
	if(IsPositionInNaturalGravity(targetCoords) == false){
		
		var forwardDir = Vector3D.Normalize(myPosition - targetCoords);
		var upDir = Vector3D.CalculatePerpendicularVector(forwardDir);
		var matrixCenter = forwardDir * 500 + targetCoords;
		targetCheckMatrix = MatrixD.CreateWorld(matrixCenter, forwardDir, upDir);
		
	}else{
		
		var forwardDir = Vector3D.Normalize(targetCoords - planetPosition);
		var upDir = Vector3D.CalculatePerpendicularVector(forwardDir);
		var matrixCenter = forwardDir * 500 + targetCoords;
		targetCheckMatrix = MatrixD.CreateWorld(matrixCenter, forwardDir, upDir);
		
	}
	
	var targetOffet = new Vector3D((double)rnd.Next(-150, 150),(double)rnd.Next(-150, 150),0);
	return Vector3D.Transform(targetOffet, targetCheckMatrix);
	
}

bool NoticableMovingTarget(long entityId){
	
	var scannedData = GetMDEI(entityId);
	
	if(scannedData.IsEmpty() == true){
		
		return false;
		
	}
	
	var speed = scannedData.Velocity.Length();
	
	if(speed &lt; 5){
		
		return false;
		
	}
	
	var blockCount = TargetBlockCount(entityId);
	
	if((double)blockCount * speed &gt; movementDetectionTrigger){
		
		return true;
		
	}
	
	return false;
	
}

