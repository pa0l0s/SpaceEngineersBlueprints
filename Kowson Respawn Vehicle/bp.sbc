{
Vector3 value=Vector3D.Zero;
if(Obj.IsAttached)
{
Vector3 v=Obj.Top.GetPosition()-anchor.CenterOfMass;
value=anchor.GetShipVelocities().LinearVelocity+anchor.GetShipVelocities().AngularVelocity.Cross(v);
}
return value;
}
