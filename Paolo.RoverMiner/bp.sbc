RC.CubeGrid.WorldVolume.Center.X,RC.CubeGrid.WorldVolume.Center.Y,RC.CubeGrid.WorldVolume.Center.Z);
++WaypointNum;
ADPrev=UserInput.AD;
}
}
else if(EnableState)
GPSTrackList.AppendFormat("GPS:{0} #{1:D3}:{2:F2}:{3:F2}:{4:F2}:\n",RC.CubeGrid.CustomName,WaypointNum,
RC.CubeGrid.WorldVolume.Center.X,RC.CubeGrid.WorldVolume.Center.Y,RC.CubeGrid.WorldVolume.Center.Z);
